<DataSpecification>
	<Grid minx="0" miny="0" maxx="40" maxy="30" w="1" l="1"/>
	<Workers size="20">
		<Location>
			<Distribution name="Uniform2D" prob="-1">
				<Settings>
					<minx value="0" />
					<miny value="0" />
					<maxx value="40" />
					<maxy value="30" />
				</Settings>
			</Distribution>
		</Location>
		<ReleaseTime mode="InterArrival">
			<Distribution name="Exponential" prob="1">
				<Settings>
					<mean value="4"/>
				</Settings>
			</Distribution>
		</ReleaseTime>
		<Duration>
			<Distribution name="Uniform" prob="1">
				<Settings>
					<min value="10"/>
					<max value="30"/>
				</Settings>
			</Distribution>
		</Duration>
		<MaxTasks>
			<Distribution name="Uniform" prob="1">
				<Settings>
					<min value="3"/>
					<max value="6"/>
				</Settings>
			</Distribution>
		</MaxTasks>
	</Workers>
	<Tasks size="100">
		<Location>
			<Distribution name="BivariateNormal" prob="0.1">
				<Settings>
					<mean1 value="23" />
					<sd1 value="1" />
					<mean2 value="14" />
					<sd2 value="1.5" />
					<covar value="" />
				</Settings>
			</Distribution>
			<Distribution name="Uniform2D" prob="-1">
				<Settings>
					<minx value="0" />
					<miny value="0" />
					<maxx value="40" />
					<maxy value="30" />
				</Settings>
			</Distribution>
		</Location>
		<ReleaseTime mode="InterArrival">
			<Distribution name="Exponential" prob="1">
				<Settings>
					<mean value="1"/>
				</Settings>
			</Distribution>
		</ReleaseTime>
		<Duration>
			<Distribution name="Uniform" prob="1">
				<Settings>
					<min value="2"/>
					<max value="6"/>
				</Settings>
			</Distribution>
		</Duration>
		<Value>
			<Distribution name="Uniform" prob="1">
				<Settings>
					<min value="1"/>
					<max value="10"/>
				</Settings>
			</Distribution>
		</Value>
	</Tasks>
</DataSpecification>
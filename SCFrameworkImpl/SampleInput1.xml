<DataSpecification>
	<Workers size="50">
		<Location>
			<Distribution name="BivariateNormal" prob="-1">
				<Settings>
					<mean1 value="3" />
					<sd1 value="1" />
					<mean2 value="4" />
					<sd2 value="1.5" />
					<covar value="" />
				</Settings>
			</Distribution>
			<Distribution name="Uniform2D" prob="-1">
				<Settings>
					<minx value="3" />
					<miny value="1" />
					<maxx value="10" />
					<maxy value="5" />
				</Settings>
			</Distribution>
		</Location>
		<ReleaseTime>
			<Distribution name="Normal" prob="1">
				<Settings>
					<mean value="5"/>
					<sd value="1"/>
				</Settings>
			</Distribution>
		</ReleaseTime>
		<Duration>
			<Distribution name="Uniform" prob="1">
				<Settings>
					<min value="2"/>
					<max value="6"/>
				</Settings>
			</Distribution>
		</Duration>
		<MaxTasks>
			<Distribution name="Constant" prob="1">
				<Settings>
					<mean value="3"/>
				</Settings>
			</Distribution>
		</MaxTasks>
	</Workers>
</DataSpecification>